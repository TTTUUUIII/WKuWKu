<?xml version='1.0' encoding='utf-8' standalone='yes' ?>
<em-config>
    <systems>
        <system
            name="Play Station"
            tag="playstation"
            manufacturer="sony" />
    </systems>
    <content-extensions>
        <item>cue</item>
        <item>bin</item>
        <item>chd</item>
        <item>img</item>
        <item>iso</item>
        <item>mdf</item>
        <item>pbp</item>
        <item>toc</item>
        <item>cbn</item>
    </content-extensions>
    <options>
        <option
            key="pcsx_rearmed_frameskip"
            defaultValue="0"
            title="Frame skip">
            <allow-values>
                <value>0</value>
                <value>1</value>
                <value>2</value>
                <value>3</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_bios"
            defaultValue="auto"
            title="Use BIOS">
            <allow-values>
                <value>auto</value>
                <value>HLE</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_region"
            defaultValue="auto"
            title="Region">
            <allow-values>
                <value>auto</value>
                <value>NTSC</value>
                <value>PAL</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_memcard2"
            defaultValue="disabled"
            title="Enable second memory card">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_input_sensitivity"
            defaultValue="1.00"
            inputType="numberDecimal"
            title="Emulated Mouse Sensitivity (0.05-2.00)" />
        <option
            key="pcsx_rearmed_multitap"
            defaultValue="disabled"
            title="Multitap Mode">
            <allow-values>
                <value>disabled</value>
                <value>port 1 only</value>
                <value>port 2 only</value>
                <value>both</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_3ds_appcputime"
            defaultValue="100"
            inputType="number"
            title="3DS app CPU time" />
        <option
            key="pcsx_rearmed_negcon_deadzone"
            defaultValue="0"
            title="NegCon Twist Deadzone">
            <allow-values>
                <value>0</value>
                <value>5</value>
                <value>10</value>
                <value>15</value>
                <value>20</value>
                <value>25</value>
                <value>30</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_negcon_response"
            defaultValue="linear"
            title="NegCon Twist Response">
            <allow-values>
                <value>linear</value>
                <value>quadratic</value>
                <value>cubic</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_analog_axis_modifier"
            defaultValue="circle"
            title="Analog axis bounds">
            <allow-values>
                <value>circle</value>
                <value>square</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_gunconadjustx"
            defaultValue="0"
            inputType="numberSigned"
            title="Guncon adjust X (-15 - 25)" />
        <option
            key="pcsx_rearmed_gunconadjusty"
            defaultValue="0"
            inputType="numberSigned"
            title="Guncon adjust Y (-15 - 25)" />
        <option
            key="pcsx_rearmed_gunconadjustratiox"
            defaultValue="1"
            inputType="numberDecimal"
            title="Guncon adjust ratio X (0.75 - 1.25)" />
        <option
            key="pcsx_rearmed_gunconadjustratioy"
            defaultValue="1"
            inputType="numberDecimal"
            title="Guncon adjust ratio Y (0.75 - 1.25)" />
        <option
            key="pcsx_rearmed_vibration"
            defaultValue="disabled"
            title="Enable vibration">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_dithering"
            defaultValue="disabled"
            title="Enable dithering">
            <allow-values>
                <value>disabled</value>
                <value>force</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_duping_enable"
            defaultValue="disabled"
            title="Frame duping">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_display_internal_fps"
            defaultValue="disabled"
            title="Display internal FPS">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_gpu_thread_rendering"
            defaultValue="disabled"
            title="GPU thread rendering">
            <allow-values>
                <value>disabled</value>
                <value>sync</value>
                <value>async</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_show_bios_bootlogo"
