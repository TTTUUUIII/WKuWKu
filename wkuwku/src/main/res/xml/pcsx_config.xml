            <allow-values>
                <value>simple</value>
                <value>gaussian</value>
                <value>cubic</value>
                <value>off</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_async_cd"
            defaultValue="sync"
            title="CD access method">
            <allow-values>
                <value>sync</value>
                <value>async</value>
                <value>precache</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_noxadecoding"
            defaultValue="disabled"
            title="XA decoding">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_nocdaudio"
            defaultValue="disabled"
            title="CD audio">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
