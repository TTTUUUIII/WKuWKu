                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_rgb32_output"
            defaultValue="disabled"
            enable="false"
            title="RGB32 output">
            <allow-values>
                <value>disabled</value>
                <value>enabled</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_crosshair1"
            defaultValue="disabled"
            title="Crosshair 1">
            <allow-values>
                <value>disabled</value>
                <value>blue</value>
                <value>green</value>
                <value>red</value>
                <value>white</value>
            </allow-values>
        </option>
        <option
            key="pcsx_rearmed_crosshair2"
            defaultValue="disabled"
