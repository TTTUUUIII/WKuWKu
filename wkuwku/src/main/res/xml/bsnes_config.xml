<?xml version="1.0" encoding="utf-8"?>
<em-config>
    <systems>
        <system
            name="Super Nintendo Entertainment System"
            tag="snes"
            manufacturer="nintendo" />
    </systems>
    <content-extensions>
        <item>sfc</item>
        <item>smc</item>
        <item>bml</item>
    </content-extensions>
    <options>
        <option
            key="bsnes_aspect_ratio"
            defaultValue="Auto"
            title="Aspect ratio">
            <allow-values>
                <value>Auto</value>
                <value>1:1</value>
                <value>4:3</value>
                <value>NTSC</value>
                <value>PAL</value>
            </allow-values>
        </option>
        <option
            key="bsnes_ppu_overscan_v"
            defaultValue="0"
            title="PPU V overscan">
            <allow-values>
                <value>0</value>
                <value>8</value>
                <value>12</value>
                <value>16</value>
            </allow-values>
        </option>
        <option
            key="bsnes_blur_emulation"
            defaultValue="OFF"
            title="Blur emulation">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_hotfixes"
            defaultValue="OFF"
            title="Hotfixes">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_entropy"
            defaultValue="None"
            title="Entropy">
            <allow-values>
                <value>None</value>
                <value>Low</value>
                <value>High</value>
            </allow-values>
        </option>
        <option
            key="bsnes_cpu_overclock"
            defaultValue="100"
            inputType="number"
            title="CPU overclock" />
        <option
            key="bsnes_cpu_sa1_overclock"
            defaultValue="100"
            inputType="number"
            title="CPU SA1 overclock" />
        <option
            key="bsnes_cpu_sfx_overclock"
            defaultValue="100"
            inputType="number"
            title="CPU SFX overclock" />
        <option
            key="bsnes_cpu_fastmath"
            defaultValue="OFF"
            title="Fast math">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_ppu_fast"
            defaultValue="OFF"
            title="Fast PPU">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_ppu_deinterlace"
            defaultValue="OFF"
            title="PPU deinterlace">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_ppu_no_sprite_limit"
            defaultValue="OFF"
            title="PPU no sprite limit">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_ppu_no_vram_blocking"
            defaultValue="OFF"
            title="PPU no VRAM blocking">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_mode7_scale"
            defaultValue="0"
            inputType="number"
            title="Mode 7 scale" />
        <option
            key="bsnes_mode7_perspective"
            defaultValue="OFF"
            title="Mode 7 perspective">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_mode7_supersample"
            defaultValue="OFF"
            title="Mode 7 supersample">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_mode7_mosaic"
            defaultValue="OFF"
            title="Mode 7 mosaic">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_dsp_fast"
            defaultValue="OFF"
            title="DSP fast">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_dsp_cubic"
            defaultValue="OFF"
            title="DSP cubic">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_dsp_echo_shadow"
            defaultValue="OFF"
            title="DSP echo shadow">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_coprocessor_delayed_sync"
            defaultValue="OFF"
            title="Coprocessor delayed sync">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_coprocessor_prefer_hle"
            defaultValue="OFF"
            title="Coprocessor prefer HLE">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_sgb_bios"
            defaultValue=""
            inputType="text"
            title="Super game boy BIOS" />
        <option
            key="bsnes_run_ahead_frames"
            defaultValue="OFF"
            title="Run ahead frames">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_touchscreen_lightgun"
            defaultValue="ON"
            title="Touchscreen lightgun">
            <allow-values>
                <value>ON</value>
                <value>OFF</value>
            </allow-values>
        </option>
        <option
            key="bsnes_touchscreen_lightgun_superscope_reverse"
            defaultValue="OFF"
            title="Touchscreen lightgun superscope reverse">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_hide_sgb_border"
            defaultValue="OFF"
            title="Hide SGB border">
            <allow-values>
                <value>OFF</value>
                <value>ON</value>
            </allow-values>
        </option>
        <option
            key="bsnes_video_filter"
            defaultValue="None"
            title="Video filter">
            <allow-values>
                <value>None</value>
                <value>NTSC (RF)</value>
                <value>NTSC (Composite)</value>
                <value>NTSC (S-Video)</value>
                <value>NTSC (RGB)</value>
            </allow-values>
        </option>
        <option
            key="bsnes_video_luminance"
            defaultValue="100"
            inputType="number"
            title="Video luminance" />
        <option
            key="bsnes_video_saturation"
            defaultValue="100"
            inputType="number"
            title="Video saturation" />
        <option
            key="bsnes_video_gamma"
            defaultValue="100"
            inputType="number"
            title="Video gamma" />

    </options>
</em-config>